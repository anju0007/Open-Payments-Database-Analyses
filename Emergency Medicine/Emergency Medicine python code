      "source": [
        "for i, row in enumerate(npi_specialty.itertuples()) :\n",
        "    for j  in range(21, 49, 2) :\n",
        "        if npi_specialty.iloc[i,j+1]  == \"Y\" :\n",
        "            code = npi_specialty.iloc[i,j]\n",
        "        \n",
        "    for n in range(0, len(special)) : \n",
        "        if special.iloc[n, 0] == code: \n",
        "            classification = special.iloc[n,2] \n",
        "            specialization = special.iloc[n,3]\n",
        "            note = special.iloc[n,8]\n",
        "\n",
        "    npi_specialty.iloc[i, 56] = classification\n",
        "    npi_specialty.iloc[i, 57] = specialization\n",
        "    npi_specialty.iloc[i, 58] = note\n",
        "npi_specialty"
